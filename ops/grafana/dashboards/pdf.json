{
  "dashboard": {
    "id": null,
    "title": "PDF Generation Performance",
    "tags": ["pdf", "performance", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "P95 Latency (Current)",
        "type": "singlestat",
        "targets": [
          {
            "expr": "job:pdf_duration_seconds:p95",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "valueFontSize": "80%",
        "format": "s",
        "decimals": 2,
        "thresholds": "3,6",
        "colorBackground": true,
        "colorValue": false,
        "colors": ["#299c46", "#e5ac0e", "#d44a3a"],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "PDF Generation Latency",
        "type": "timeseries",
        "targets": [
          {
            "expr": "job:pdf_duration_seconds:p50",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "job:pdf_duration_seconds:p95",
            "legendFormat": "P95",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10,
              "gradientMode": "none"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 9,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "PDF Error Rate (%)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "job:pdf_error_rate5m * 100",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 20,
              "gradientMode": "none"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 9,
          "x": 15,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Signed URL Expiry Rate (%)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "(sum(rate(expired_signed_url_encounters_total[15m])) / clamp_min(sum(rate(pdf_requests_total{route=\"save\",status=\"ok\"}[15m])), 1)) * 100",
            "legendFormat": "TTL Expiry Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 20,
              "gradientMode": "none"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 3},
                {"color": "red", "value": 10}
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "PDF Request Volume",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(pdf_requests_total{route=\"download\"}[5m]))",
            "legendFormat": "Download Requests/sec",
            "refId": "A"
          },
          {
            "expr": "sum(rate(pdf_requests_total{route=\"save\"}[5m]))",
            "legendFormat": "Save Requests/sec",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10,
              "gradientMode": "none"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "links": []
  }
}