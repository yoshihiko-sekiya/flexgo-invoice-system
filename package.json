{
  "name": "mentor-lite",
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173 --strictPort",
    "build": "vue-tsc && vite build",
    "preview": "vite preview --port 5173 --strictPort",
    "dev:api": "tsx server/index.ts",
    "dev:all": "concurrently -k \"npm run dev\" \"npm run dev:api\"",
    "port:free": "kill-port 5173 || true",
    "port:who": "node scripts/port-check.mjs",
    "predev": "npm run port:free",
    "pree2e:run": "npm run port:free",
    "e2e:run": "start-server-and-test \"npm run dev:all\" http://localhost:8787/api/health \"cypress run --headless --spec cypress/e2e/reports_preview.cy.ts\"",
    "e2e:invoices": "start-server-and-test \"npm run dev:all\" http://localhost:8787/api/health \"cypress run --headless --spec cypress/e2e/invoices.cy.ts\"",
    "test:e2e": "cypress run",
    "cy:open": "cypress open",
    "cleanup": "tsx server/scripts/cleanup.ts",
    "cleanup:dry": "CLEANUP_DRY_RUN=true tsx server/scripts/cleanup.ts",
    "release": "standard-version"
  },
  "dependencies": {
    "@ionic/vue": "^8.3.2",
    "@ionic/vue-router": "^8.3.2",
    "@sentry/node": "^10.4.0",
    "@supabase/supabase-js": "^2.45.4",
    "@types/uuid": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "zod": "^3.23.8",
    "ionicons": "^8.0.13",
    "mapbox-gl": "^3.6.0",
    "prom-client": "^15.1.3",
    "puppeteer": "^22.0.0",
    "uuid": "^11.1.0",
    "vue": "^3.5.5",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@cypress/snapshot": "^2.1.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mapbox-gl": "^3.4.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "concurrently": "^8.2.2",
    "cypress": "^14.5.4",
    "kill-port": "^2.0.1",
    "standard-version": "^9.5.0",
    "start-server-and-test": "^2.0.13",
    "tsx": "^4.7.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vue-tsc": "^2.1.6"
  }
}
